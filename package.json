    "@babel/preset-env": "7.10.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.1",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "cssnano": "4.1.10",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.11.0",
