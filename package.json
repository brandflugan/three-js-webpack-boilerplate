{
  "name": "three-js-webpack-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandflugan/three-js-webpack-boilerplate.git"
  },
  "version": "1.0.0",
  "description": "Webpack 4 Three.js Boilerplate",
  "main": "index.js",
